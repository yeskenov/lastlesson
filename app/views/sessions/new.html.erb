  <div class="col-sm-offset-3 col-sm-6">
      <%= form_for :session, url: login_path do |f| %>
            <div class="form-group">
              <%= f.label :email %>
              <%= f.email_field :email,class:"form-control", rows:10, placeholder:"Email" %>
            </div>
            <div class="form-group">
              <%= f.label :password %>
              <%= f.password_field :password,class:"form-control", rows:10, placeholder:"Password",autocomplete: false %>
              
            </div>
            <%= f.submit "Log in", :class => "btn btn-lg btn-success pull-right"%>
      <% end %>
  </div>